{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "#seedworks/*": [
        "./@seedwork/*"
      ],
      "#category/*": [
        "./category/*"
      ]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": [
    "src/"
  ]
}
